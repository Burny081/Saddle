# Saddle Point Service - Environment Variables
# Copy this file to .env.local and fill in your values

# Application
VITE_APP_NAME=Saddle Point Service
VITE_APP_VERSION=0.0.1
VITE_APP_DESCRIPTION=Premium Dashboard for Commercial Management

# API Configuration
VITE_API_URL=http://localhost:3001/api
VITE_API_TIMEOUT=30000

# Geolocation API (for visitor tracking)
VITE_GEO_API_URL=https://ipapi.co/json/

# Feature Flags
VITE_FEATURE_PDF_EXPORT=true
VITE_FEATURE_ADVANCED_REPORTS=true
VITE_FEATURE_MULTI_LANGUAGE=true
VITE_FEATURE_CHAT=true

# Logging
VITE_LOG_LEVEL=info
VITE_DEBUG_MODE=false
